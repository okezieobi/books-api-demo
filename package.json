{
  "name": "books-api-demo",
  "version": "1.0.0",
  "description": "A simple book api demo",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development DEBUG=books-api-demo:* nodemon src/server.ts",
    "release": "npm run clean && npm run build-app && npm run start",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:app",
    "build:app": "npx tsc",
    "start": "node dist/server.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okezieobi/books-api-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okezieobi/books-api-demo/issues"
  },
  "homepage": "https://github.com/okezieobi/books-api-demo#readme",
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^0.27.2",
    "better-ajv-errors": "^1.2.0",
    "cors": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "morgan": "latest",
    "pg-promise": "^10.11.1",
    "swagger-jsdoc": "latest",
    "swagger-ui-express": "latest",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/cors": "latest",
    "@types/debug": "latest",
    "@types/express": "latest",
    "@types/morgan": "latest",
    "@types/node": "latest",
    "@types/swagger-jsdoc": "latest",
    "@types/swagger-ui-express": "latest",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.18",
    "prettier": "^2.6.2",
    "prettier-config-airbnb": "^1.0.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint --cache --fix",
    "**/*.{js,css,md,ts}": "prettier --write, --ignore-unknown"
